{% extends 'base.html.twig' %}

{% block title %}Add new list{% endblock %}

{% block body %}
    <div class="container">
        <div class="row">
            <div class="bg-light mt-4 p-4">
                <h1>Add new list</h1>

                {{ form_start(form) }}
                {{ form_row(form.name) }}

                <button type="submit" class="btn btn-primary">Add</button>
                {{ form_end(form) }}
            </div>
        </div>
    </div>
{% endblock %}